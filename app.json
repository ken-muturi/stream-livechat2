{
  "expo": {
    "name": "churchStream",
    "slug": "churchStream",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "codes.stadi.churchstream"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store",
      [
        "expo-barcode-scanner", {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera to scan book"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
